{
  "rewrites": [
    {
      "source": "/payment/:id",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(bot|spider|crawler|curl|httpie|insomnia|postman|wget|axios|fcbot).*"
        }
      ],
      "destination": "https://frames.payflow.me/payment/:id"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
